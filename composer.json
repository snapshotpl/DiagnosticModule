{
    "name": "snapshotpl/diagnostic-module",
    "description": "ZendDiagnostic module for ZF2 and ZF3",
    "type": "library",
    "require": {
        "php": ">=5.6",
        "zendframework/zenddiagnostics": "^1.1",
        "psr/container": "^1.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Snapshotpl\\DiagnosticModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Snapshotpl\\DiagnosticModule\\": "tests/"
        }
    }
}
