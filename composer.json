{
    "name": "snapshotpl/diagnostic-module",
    "description": "ZendDiagnostic module for ZF2 and ZF3",
    "type": "library",
    "require": {
        "php": "^7.0",
        "zendframework/zend-mvc": "^2.7 || ^3.0",
        "zendframework/zenddiagnostics": "^1.0",
        "zendframework/zend-mvc-console": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Snapshotpl\\DiagnosticModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Snapshotpl\\DiagnosticModule\\": "tests/"
        }
    }
}
